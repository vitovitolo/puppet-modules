<VirtualHost *:80>

ServerName grafana
ServerAlias grafana.<%=@domain%>

	<Location "/">
		ProxyPass "http://localhost:<%=@http_port%>/"
		ProxyPassReverse "http://localhost:<%=@http_port%>/"
	</Location>

	ErrorLog ${APACHE_LOG_DIR}/grafana_error.log
	LogLevel warn
	CustomLog ${APACHE_LOG_DIR}/grafana_access.log combined
</VirtualHost>
